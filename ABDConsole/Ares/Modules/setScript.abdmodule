source ~/Ares/aresbd.conf
echo "Loading setScript.abdmodule..."
setScript()
{
	cd $ARESSCRIPTROOT
	PS3=$SCRIPTPROMPT
	select SCRIPT in *; do
		PS3=$MENUPROMPT
		cd $ARESROOT
	break
done
}echo "Done."

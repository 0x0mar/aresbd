source ~/Ares/aresbd.conf
echo "Loading attackShell.abdmodule..."
attackShell()
{
	if [[ -z "$PORT" ]]; then
		echo "You need to set the port first!"
	else
		nc -l -p $PORT
	fi
}echo "Done."

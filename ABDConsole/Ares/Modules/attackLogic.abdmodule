echo "Loading attackLogic.abdmodule... \c"
attackLogic()
{
	if [[ -z "$ATTACKMODE" ]]; then
	echo "You need to set an attack mode first!"
	fi
	
	if [[ $ATTACKMODE = "Shell" ]]; then
		attackShell
	fi
	
	if [[ $ATTACKMODE = "Script" ]]; then
		attackScript
	fi
}
echo "Done."

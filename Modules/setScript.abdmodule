echo -e "Loading setScript.abdmodule... \c"
setScript()
{
	cd $ARESSCRIPTROOT
	PS3=$SCRIPTPROMPT
	select SCRIPT in *; do
		PS3=$MENUPROMPT
		cd $ARESROOT
	break
done
}
echo "Done."
